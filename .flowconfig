[ignore]
.*/dist/.*
.*/build/.*
.*/examples/.*
.*/node_modules/bab.*
.*/node_modules/lodash.*
.*/node_modules/array.*
.*/node_modules/is-.*
.*/node_modules/object.*
.*/node_modules/acorn.*

[libs]
./lib

[options]
munge_underscores=true
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
module.name_mapper='.*\(\.css\)' -> 'empty/object'
module.name_mapper='.*\(\.configloaderrc\)' -> 'empty/object'

module.name_mapper='^todomvc/\(.*\)' -> '<PROJECT_ROOT>/src/\1'
