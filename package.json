{
  "name": "rdi-examples",
  "version": "1.0.22",
  "description": "Reactive di examples",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "."
  },
  "iife:main": "docs/examples/app.js",
  "scripts": {
    "release": "npm test && npm run build && standard-version -a && npm run postversion",
    "postversion": "git push --follow-tags origin master && npm publish",
    "build": "rimraf docs/examples && rollup -c",
    "start": "rimraf docs/examples && rollup -c -w --config rollup.config.examples.js"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di-examples.git"
  },
  "keywords": [
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.31",
    "@babel/plugin-external-helpers": "^7.0.0-beta.31",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.31",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.31",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.31",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.31",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.31",
    "@babel/preset-es2015": "^7.0.0-beta.31",
    "@babel/register": "^7.0.0-beta.31",
    "babel-plugin-transform-metadata": "^4.0.1",
    "flow-bin": "^0.58.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^0.3.1",
    "rollup-watch": "^4.3.1",
    "standard-version": "^4.2.0",
    "uglify-es": "^3.1.6"
  },
  "dependencies": {
    "fetch-mock": "^5.13.1",
    "jss": "^9.1.0",
    "jss-camel-case": "^6.0.0",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "lom_atom": "^2.0.7",
    "preact": "^8.2.6",
    "reactive-di": "^5.0.3",
    "todomvc-common": "^1.0.4"
  }
}
