{
  "name": "reactive-di-todomvc",
  "version": "0.0.1",
  "description": "Reactive di - todomvc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "config": {
    "builddir": "."
  },
  "scripts": {
    "postinstall": "test -d conf && conf/resources/postinstall.sh $npm_package_name || exit 0",
    "prepush": "exit 0 && npm test",
    "prepublish": "conf/resources/prepublish.sh",
    "preversion": "conf/resources/checkgit.sh && npm test",
    "postversion": "git push && git push --tags",
    "precommit": "exit 0",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf build coverage",
    "build.babel": "npm run clean && babel src --source-maps --out-dir $npm_package_config_builddir/dist",
    "build": "npm run clean && webpack --config conf/webpack.config.es5.js",
    "build.dev": "node conf/resources/rundeps.js reactive-di-observable reactive-di reactive-di-react",
    "lint": "eslint src",
    "check": "flow check",
    "testonly": "mocha",
    "test": "npm run lint && npm run check && npm run testonly",
    "test.dev": "npm run testonly -- --growl --watch",
    "test.cov": "babel-istanbul cover --report text --report html node_modules/mocha/bin/_mocha",
    "start": "webpack-dev-server --config conf/webpack.config.es5.js --hot --history-api-fallback --inline"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di-react.git"
  },
  "keywords": [
    "react",
    "reactive-di",
    "dependency injection",
    "di",
    "modular",
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "README.md",
    "LICENSE",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.1.2",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-plugin-module-alias": "^1.4.0",
    "babel-plugin-react-require": "^2.1.0",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-metadata": "^0.x.x",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "empty": "^0.10.1",
    "eslint-config-airplus": "^1.1.9",
    "find-root": "^1.0.0",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.16.2",
    "import-glob-loader": "^1.1.0",
    "mocha": "^2.4.5",
    "node-config-loader": "^2.0.7",
    "postcss-loader": "^0.9.1",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.4",
    "source-map-loader": "git+https://github.com/zerkalica/source-map-loader.git",
    "style-loader": "^0.13.1",
    "todomvc-app-css": "^2.0.5",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "any-translate": "^0.x.x",
    "any-translate-adapter-babelfish": "^0.x.x",
    "classnames": "^2.2.5",
    "es6-error": "^2.1.1",
    "modern-router": "0.1.5",
    "node-config-loader": "^2.0.7",
    "observable-helpers": "^0.x.x",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "reactive-di": "^1.1.8",
    "reactive-di-observable": "^1.x.x",
    "reactive-di-react": "^1.x.x",
    "zen-observable": "^0.x.x"
  }
}
