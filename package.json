{
  "name": "reactive-di-todomvc",
  "version": "0.0.1",
  "description": "Reactive di - todomvc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "builddir": "."
  },
  "scripts": {
    "prepush": "npm test",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf dist",
    "build.babel": "npm run clean && babel src --source-maps --out-dir $npm_package_config_builddir/dist",
    "build": "npm run clean && webpack --config conf/webpack.config.es5.js",
    "lint": "eslint src",
    "check": "flow check",
    "testonly": "mocha",
    "test": "npm run lint && npm run check && npm run testonly",
    "test.dev": "npm run testonly -- --growl --watch",
    "start": "webpack-dev-server --config conf/webpack.config.es5.js --hot --history-api-fallback --inline"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di-react.git"
  },
  "keywords": [
    "react",
    "reactive-di",
    "dependency injection",
    "di",
    "modular",
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "README.md",
    "LICENSE",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-metadata": "^1.x.x",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cookies-js": "^1.2.2",
    "css-loader": "^0.25.0",
    "empty": "^0.10.1",
    "eslint": "^3.8.1",
    "eslint-config-airplus": "^2.0.6",
    "file-loader": "^0.9.0",
    "find-root": "^1.0.0",
    "glob": "^7.1.1",
    "globby": "^6.0.0",
    "html-webpack-plugin": "^2.24.0",
    "husky": "^0.11.9",
    "inert": "^4.0.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.6",
    "source-map-loader": "git+https://github.com/zerkalica/source-map-loader.git",
    "style-loader": "^0.13.1",
    "vision": "^4.1.0",
    "webpack": "^1.x.x",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "bowser": "^1.4.6",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "es6-error": "^4.0.0",
    "fetch-builder": "^1.1.7",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^5.5.5",
    "jss-camel-case": "^2.0.0",
    "jss-compose": "^1.1.0",
    "jss-nested": "^2.4.3",
    "modern-router": "^1.1.4",
    "node-config-loader": "^3.0.6",
    "observable-helpers": "^0.1.2",
    "querystring-browser": "^1.0.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "reactive-di": "^2.3.9",
    "shortid": "^2.2.6"
  }
}
