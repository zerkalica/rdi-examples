{
  "name": "reactive-di-todomvc",
  "version": "2.0.0",
  "description": "Reactive di - todomvc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "builddir": "."
  },
  "scripts": {
    "prepush": "npm test",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf docs",
    "build.babel": "npm run clean && babel modules --source-maps --out-dir $npm_package_config_builddir/dist",
    "build": "npm run clean && webpack --config conf/webpack.config.babel.js",
    "build.dep": "npm-helpers/build-deps.sh",
    "lint": "eslint src",
    "check": "flow check",
    "testonly": "mocha",
    "test": "npm run lint && npm run check && npm run testonly",
    "test.dev": "npm run testonly -- --growl --watch",
    "rdi": "cd ../reactive-di && npm run build && cp -rf dist ../reactive-di-todomvc/node_modules/reactive-di",
    "start": "webpack-dev-server --config conf/webpack.config.babel.js --hot --history-api-fallback --inline",
    "analyze": "webpack --config conf/webpack.config.babel.js --json | webpack-bundle-size-analyzer"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di-react.git"
  },
  "keywords": [
    "react",
    "reactive-di",
    "dependency injection",
    "di",
    "modular",
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "README.md",
    "LICENSE",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-metadata": "^2.x.x",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cookies-js": "^1.2.3",
    "css-loader": "^0.26.2",
    "empty": "^0.10.1",
    "eslint": "^3.16.1",
    "eslint-config-airplus": "^2.0.10",
    "file-loader": "^0.10.1",
    "find-root": "^1.0.0",
    "globby": "^6.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "inert": "^4.1.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.2",
    "vision": "^4.1.1",
    "webpack": "^2.x.x",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "bowser": "^1.6.0",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "es6-error": "^4.0.2",
    "fetch-builder": "^1.1.10",
    "font-awesome": "^4.7.0",
    "inferno": "^1.3.0-rc.8",
    "inferno-component": "^1.3.0-rc.8",
    "inferno-create-element": "^1.3.0-rc.8",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^6.3.0",
    "jss-camel-case": "^3.0.0",
    "jss-compose": "^2.0.0",
    "jss-nested": "^3.0.1",
    "modern-router": "^2.0.1",
    "node-config-loader": "^3.0.8",
    "observable-helpers": "^0.1.2",
    "querystring-browser": "^1.0.4",
    "reactive-di": "^3.1.5",
    "shortid": "^2.2.6"
  }
}
