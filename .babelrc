{
    "presets": ["es2015-loose", "stage-0", "react"],
    "plugins": [
        "syntax-flow",
        ["module-resolver", {
            "root": ["", "./modules"],
            "alias": {
                "rdi-todo": "./src"
            }
        }],
        ["transform-metadata", {
            "jsxPragma": "__h",
            "typeNameStrategy": "typeName",
            "reflectImport": "reactive-di/CustomReflect"
        }],
        ["transform-react-jsx", {
            "pragma": "__h"
        }],
        "transform-decorators-legacy",
        "transform-flow-strip-types"
    ],
    "env": {
        "development": {
            "plugins": [
                "espower"
            ]
        }
    }
}
